{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": [],
    "paths": {
      "@shared": [ "app/shared" ],
      "@shared/*": [ "app/shared/*" ],
      "@core": [ "app/core/" ],
      "@core/*": [ "app/core/*" ],
      "@pages": [ "app/pages/" ],
      "@pages/*": [ "app/pages/*" ],
      "@zorro-ext": [ "app/zorro-ext/" ],
      "@zorro-ext/*": [ "app/zorro-ext/*" ],
      "@env": [ "environments/" ],
      "@env/*": [ "environments/*" ]
  }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
